<UserControl x:Class="PC_Verwaltung.Register"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PC_Verwaltung"
             mc:Ignorable="d" 
             d:DesignHeight="{StaticResource Height}" 
             d:DesignWidth="{StaticResource Width}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label x:Name="label" Content="Not implemented yet" HorizontalAlignment="Center" VerticalAlignment="Bottom" Grid.Row="0" Margin="10" FontSize="36"/>
        <Button x:Name="button" Content="OK" Click="ButtonClick" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Margin="10" FontSize="36" Width="91"/>
    </Grid>
</UserControl>
